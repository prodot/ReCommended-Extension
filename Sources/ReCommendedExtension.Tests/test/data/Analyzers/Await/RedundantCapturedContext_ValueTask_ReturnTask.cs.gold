            {
                await ValueTasks.Delay(10);
            }

            |await|(1) ValueTasks.Delay(20);

            int LocalFunction()
            {
                return 4;
            }
        }

        async Task Method3() => |await|(2) ValueTasks.Delay(10);

        async Task<int> Method4()
        {
